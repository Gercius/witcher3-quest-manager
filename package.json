        "css": "sass --watch src/styles/sass:src/styles/css",
        "extract-quests": "node data/scripts/extract-quest-data.js"
    },
    "devDependencies": {
        "concurrently": "^9.1.0",
        "exceljs": "^4.4.0",
        "sass": "^1.81.0",
        "typescript": "~5.6.2",
        "vite": "^5.4.10",
        "vite-plugin-html": "^3.2.2"
    }
}
